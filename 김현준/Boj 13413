for i in range(int(input())):
    k=int(input())
    a=list(input())
    b=list(input())
    A=0
    B=0
    for j in range(len(a)):
        if a[j]=='W' and a[j]!=b[j]:
            A+=1
        elif a[j]=='B' and a[j]!=b[j]:
            B+=1
    if A>=B:
        print(A)
    elif B>A:
        print(B)
