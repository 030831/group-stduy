a=input()
b=0
for i in range(len(a)):
    if a[i]=='U':
        for j in range(i+1,len(a)):
            if a[j]=='C':
                for k in range(j+1,len(a)):
                    if a[k]=='P':
                        for m in range(k+1,len(a)):
                            if a[m]=='C':
                                b=1
if b==1:
    print("I love UCPC")
elif b==0:
    print("I hate UCPC")
